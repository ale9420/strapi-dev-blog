{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "published": {
      "type": "datetime"
    },
    "featured_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "content": {
      "type": "blocks"
    },
    "excerpt": {
      "type": "blocks",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "meta_description": {
      "type": "text"
    },
    "meta_title": {
      "type": "string"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    },
    "primary_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::author.author"
    }
  }
}
